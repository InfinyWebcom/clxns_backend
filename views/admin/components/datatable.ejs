<script>
    <%if(locals.ViewUrl) { %>
    const ViewUrl = '<%=ViewUrl%>';
    <% } %>
    <%if(locals.EditUrl) { %>
    const EditUrl = '<%=EditUrl%>';
    <% } %>
    <%if(locals.DelUrl) { %>
    const DelUrl = '<%=ViewUrl%>';
    <% } %>
