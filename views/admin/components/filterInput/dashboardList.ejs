

<div>
  <h6 class="mt-5 pb-0 mb-0">FI Name</h6>
  <div class="navbar-options row">
      <div class="input-field col s12 navbar-color mb-0 mt-0 pt-0">
          <div class="center-align row">
              <% var FIname = ['HDFC','ICICI','KREZYBEE','AXIX']; %>
                  <% FIname.forEach(function(FIs,i){ %>
                  <div class="input-field col s12 mt-0 pb-5">
                      <label>
                          <input type="checkbox" name="FIs[]" value="<%= FIs %>">
                          <span>
                              <%= FIs %>
                          </span>
                      </label>
                  </div>
                  <% }); %>
          </div>

      </div>
  </div>
  <h6 class="mt-3" style="margin-top: 50px !important;">From Date</h6>
  <div class="navbar-options row ">
      <div class="input-field col s12 navbar-color mb-0 mt-0 pt-0 ">
          <div class=" center-align">
            <input type="date" id="fromdate" name="fromdate" placeholder="From Date">
          </div>

      </div>
  </div>
  <h6 class="mt-3">To Date</h6>
  <div class="navbar-options row ">
      <div class="input-field col s12 navbar-color mb-0 mt-3 pt-0 ">
          <div class=" center-align">
            <input type="date" id="todate" name="todate" placeholder="To Date">
          </div>

      </div>
  </div>
</div>
