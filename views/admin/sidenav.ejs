              <i class="material-icons">radio_button_unchecked</i>
              <span>Fresh</span>
            </a>
          </li>
          <li>
            <a href="/leads/assignLeads"
              class="<% if(url.includes(`assignLeads`)){ %> active gradient-45deg-green-teal <% } %>">
              <i class="material-icons">radio_button_unchecked</i>
