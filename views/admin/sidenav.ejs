      </a>
    </li>
    <% } %> -->

    <% if (permissions.includes('reports') || roleName=='FiName') { %>
    <li class="<% if(url.includes(`reports`)){ %> active <% } %> bold">
      <a class="waves-effect waves-cyan <% if(url.includes(`reports`)){ %> active gradient-45deg-green-teal <% } %>"
