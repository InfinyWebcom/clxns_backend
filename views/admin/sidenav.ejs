        <i class="material-icons">pie_chart_outlined</i>
        <span class="menu-title" data-i18n="Mail">Reports</span>
      </a>
    </li>
    <% } %>

    <% if (permissions.includes('users')) { %>
    <li class="<% if(url.includes(`users`)){ %> active <% } %> bold">
      <a class="waves-effect waves-cyan <% if(url.includes(`user`)){ %> active gradient-45deg-green-teal <% } %>"
